@model AbInitio.Web.ViewModels.ZoekenViewModel

<div class="beheertab_content">
    <table class="table table-striped">
        <tr>
            <th>
                Voornaam
            </th>
            <th>
                Tussenvoegsel
            </th>
            <th>
                Achternaam
            </th>
            <th>
                Geboortenaam
            </th>
            <th>
                Geslacht
            </th>
            <th>
                Status
            </th>
            <th>
                Geboortedatum
            </th>
            <th>
                Geboorteprecisie
            </th>
            <th></th>
        </tr>
        @foreach (var item in ViewBag.MyList)
        {
            <tr class="persoon_row" data-href="@Url.Action("PersoonDetails", "Beheer", new { id = item.persoonid })">
                <td>
                    @item.voornaam
                </td>
                <td>
                    @item.tussenvoegsel
                </td>
                <td>
                    @item.achternaam
                </td>
                <td>
                    @item.geboortenaam
                </td>
                <td>
                    @item.geslacht
                </td>
                <td>
                    @item.geboortedatum
                </td>
                <td>
                    @item.geboorteprecisie
                </td>
            </tr>
        }
    </table>
</div>